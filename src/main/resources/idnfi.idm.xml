				<item>
					<code>4</code>
					<label>71 - 100% step 30 limit for enumeration</label>
				</item>
				<item>
					<code>5</code>
					<label>101+%</label>
				</item>
			</items>
		</list>
		<list name="aspect">
			<label type="item">Aspect</label>
			<items>
				<item>
					<code>0</code>
					<label>Flat and undulating (0 - 10%)</label>
				</item>
				<item>
					<code>1</code>
					<label>N 338 - 22 degrees</label>
				</item>
				<item>
					<code>2</code>
					<label>NE 23 - 67 degrees</label>
				</item>
				<item>
					<code>3</code>
					<label>E 68 - 112 degrees</label>
				</item>
				<item>
					<code>4</code>
					<label>SE 113 - 157 degrees</label>
				</item>
				<item>
					<code>5</code>
					<label>S 158 - 202 degrees</label>
				</item>
				<item>
					<code>6</code>
					<label>SW 203 - 247 degrees</label>
				</item>
				<item>
					<code>7</code>
					<label>W 248 - 292 degrees</label>
				</item>
				<item>
					<code>8</code>
					<label>NW 293 - 337 degrees</label>
				</item>
			</items>
		</list>
	</codeLists>
	<units>
		<unit name="deg" dimension="angle">
			<label>degrees</label>
			<abbreviation>deg</abbreviation>
		</unit>
		<unit name="m" dimension="length" conversionFactor="1.0">
			<label>metres</label>
			<abbreviation>m</abbreviation>
		</unit>
		<unit name="cm" dimension="length" conversionFactor="0.01">
			<label>centimetres</label>
			<abbreviation>cm</abbreviation>
		</unit>
		<unit name="km" dimension="length" conversionFactor="1000">
			<label>kilometre</label>
			<abbreviation>km</abbreviation>
		</unit>
		<unit name="ac" dimension="area" conversionFactor="2.47105381">
			<label>acres</label>
			<abbreviation>ac</abbreviation>
		</unit>
		<unit name="ha" dimension="area" conversionFactor="1.0">
			<label>hectares</label>
			<abbreviation>ha</abbreviation>
		</unit>
		<unit name="percent" dimension="ratio" conversionFactor="0.01">
			<label>percent</label>
			<abbreviation>%</abbreviation>
		</unit>
	</units>
	<schema>
		<entity name="cluster" ui:tab="cluster">
			<label type="instance">Cluster</label>
			<number name="utm_zone" type="integer" required="true">
				<label>UTM zone</label>
				<description>Valid UTM Zone between 46 and 54</description>
				<compare flag="error" gte="46" lte="54" />
			</number>
			<number name="easting" type="integer" required="true" key="true">
				<label>W-E</label>
				<description>Valid UTM easting between 166 and 834</description>
				<compare flag="error" gte="166" lte="834" />
			</number>
			<number name="northing" type="integer" required="true" key="true">
				<label>S-N</label>
				<description>Valid UTM northing between 0 and 750 or between 8670 and 9999</description>
				<check expr="($this &gt;= 0 and $this &lt;= 750) or ($this &gt;= 8670 and $this &lt;= 9999)" flag="error">
					<message>The northing value must be between 0 and 750 or between 8670 and 9999</message>
				</check>
			</number>
			<entity name="natural_forest" multiple="true" ui:tab="nf" ui:layout="form">
				<label type="number">1</label>
				<label type="heading">Enumeration, Natural Forest</label>
				<number name="tract_no" type="integer" required="true" key="true" ui:tab="nf_detail">
					<label type="number">3</label>
					<label>Tract</label>
					<compare flag="error" gte="1" lte="15" />
					<!-- TODO: Min/max value depends on UTM coords? -->
				</number>
				<number name="subplot_no" type="integer" required="true" key="true" ui:tab="nf_detail">
					<label>Subplot</label>
					<compare flag="error" gte="1" lte="8" />
				</number>
				<number name="sector" type="real" ui:tab="nf_detail">
					<label>Sector</label>
					<precision unit="deg" decimalDigits="0" />
				</number>
				<number name="segment_dist" type="real" relevant="sector=''" ui:tab="nf_detail">
					<label>Segment distance</label>
					<precision unit="m" decimalDigits="2" />
				</number>
				<number name="part" type="integer" required="true" relevant="sector!='' or segment_dist!=''" key="true" ui:tab="nf_detail">
					<label>Part</label> <!-- TODO: what is the code about? -->
				</number>
				<number name="permanent" type="integer" required="false" ui:tab="nf_detail">
					<label>Permanent</label>
					<compare gte="1" lte="1" flag="error">
						<message>This field must be either 1 or blank</message>
					</compare>
				</number>
				<number name="control" type="integer" required="false" key="true" ui:tab="nf_detail">
					<label>Control</label>
					<compare gte="1" lte="1" flag="error">
						<message>This field must be either 1 or blank</message>
					</compare>
				</number>
				<code name="province" required="true" list="province" ui:tab="nf_detail">
					<label>Province</label>
				</code>
				<code name="land_system" required="true" list="land_system" ui:tab="nf_detail">
					<label>Land system</label>
				</code>
				<code name="altitude" required="true" list="altitude" ui:tab="nf_detail">
					<label>Altitude</label>
				</code>
				<code name="land_category" required="true" list="land_category" ui:tab="nf_detail">
					<label>Land category</label>
				</code>
				<code name="forest_type" required="true" list="forest_class" ui:tab="nf_detail">
					<label>Forest type</label>
				</code>
				<code name="stand_condition" required="true" list="forest_class" parent="forest_type" ui:tab="nf_detail">
					<label>Stand condition</label>
				</code>
				<number name="logging_year" type="integer" required="false" ui:tab="nf_detail">
					<label type="number">18</label>
					<label>Logging year</label>
					<compare gte="1900" lte="2050" flag="error" />
				</number>
				<code name="terrain" required="true" list="terrain" ui:tab="nf_detail">
					<label>Terrain</label>
				</code>
				<code name="slope" required="true" list="slope" ui:tab="nf_detail">
					<label>Slope</label>
				</code>
				<code name="aspect" required="true" list="aspect" ui:tab="nf_detail">
					<label>Aspect</label>
				</code>
				<entity name="record_count" multiple="false" ui:tab="nf_detail">
					<label>Number of Records</label>
					<number name="trees_poles" type="integer" required="false">
						<label>Trees and poles</label>
						<default value="0" />
						<compare flag="error" gte="0" />
					</number>
					<number name="seedlings" type="integer" required="false">
						<label>Seedlings</label>
						<default value="0" />
						<compare flag="error" gte="0" />
					</number>
					<number name="saplings" type="integer" required="false">
						<label>Saplings</label>
						<default value="0" />
						<compare flag="error" gte="0" />
					</number>
					<number name="sm_rattan" type="integer" required="false">
						<label>Rattan (&lt;=2.9m)</label>
						<default value="0" />
						<compare flag="error" gte="0" />
					</number>
					<number name="lg_rattan" type="integer" required="false">
						<label>Rattan (&gt;=3m)</label>
						<default value="0" />
						<compare flag="error" gte="0" />
					</number>
				</entity>
				<number name="crew_no" type="integer" required="false" ui:tab="nf_detail">
					<label>Crew no.</label>
					<compare flag="error" gte="0" />
				</number>
				<code name="month" list="month" required="true" ui:tab="nf_detail">
					<label>Month</label>
				</code>
				<number name="year" type="integer" required="true" ui:tab="nf_detail">
					<label>Year</label>
					<compare gte="1900" lte="2050" flag="error" />
				</number>
				<entity name="tp" multiple="true" ui:tab="nf_tp">
					<label type="heading">Trees and Poles</label>
					<text name="species_name">
						<label>Name of Species</label>
					</text>
					<number name="diameter" type="real">
						<label>Diameter</label>
						<description>DBH or diameter 0.2m AB</description> <!-- TODO: Description -->
						<precision decimalDigits="1" unit="cm" />
					</number>
					<!-- TODO: Add damage codelist -->
					<!-- <code name="damage" list="damage"> -->
					<!-- <label>Damage</label> -->
					<!-- </code> -->
					<entity name="lg_trees" multiple="false" relevant="diameter &gt;= 20">
						<label>Large trees</label>
						<description>Trees with diameter &gt;=20cm</description>
						<number name="buttress_height" type="real">
							<label>Buttress height</label>
							<precision decimalDigits="2" unit="cm" />
							<compare gt="0" flag="error"/>  
						</number>
						<number name="bole_height" type="real">
							<label>Bole height</label>
							<precision decimalDigits="1" unit="cm" />
							<compare gt="0" flag="error"/>
						</number>
						<!-- TODO: Add grade and infestation codelist -->
						<!-- <code name="grade" list="grade"> -->
						<!-- <label>Grade</label> -->
						<!-- </code> -->
						<!-- <code name="infestation" list="infestation" multiple="true" maxCount="2"> -->
						<!-- <label>Infestation</label> -->
						<!-- <default value="0"/> -->
						<!-- </code> -->						
					</entity>
					<entity name="bole_height" multiple="false">
						<label>Bole height</label>
						<number name="horizontal_distance" type="real">
							<label>Horizontal distance</label>
							<precision decimalDigits="2" unit="m" />
							<compare gt="0" flag="error"/>
						</number>
						<number name="base_height" type="real">
							<label>Height of base</label>
							<precision decimalDigits="1" unit="m" />
							<compare gt="0" flag="error"/>
						</number>
						<number name="percent_base" type="real">
							<label>Percent base</label>
							<precision decimalDigits="0" unit="percent" />
							<compare gte="0" lte="100" flag="error" />
						</number>
						<number name="percent_crown_point" type="real">
							<label>Percent crown point</label>
							<precision decimalDigits="0" unit="percent" />
							<compare gte="0" lte="100" flag="error" />
						</number>
					</entity>
					<entity name="buttress" multiple="false">
						<label>Buttress</label>
						<number name="horizontal_distance" type="real">
							<label>Horizontal distance</label>
							<precision decimalDigits="2" unit="m" />
						</number>
						<number name="percent_base" type="real">
							<label>Percent base</label>
							<precision decimalDigits="0" unit="percent" />
							<compare gte="0" lte="100" flag="error" />
						</number>
						<number name="percent_buttress" type="real">
							<label>Percent buttress</label>
							<precision decimalDigits="0" unit="percent" />
							<compare gte="0" lte="100" flag="error" />
						</number>
					</entity>
					<entity name="dab" multiple="false">
						<label>DAB</label>
						<description>Diameter 0.2m above buttress</description>
						<number name="full_bars" type="real">
							<label>Full bars</label>
						</number>
						<number name="quarter_bars" type="real">
							<label>1/4 bars</label>
							<precision decimalDigits="1" unit="percent" />
						</number>
					</entity>
				</entity>
				<entity name="ssr" multiple="true" ui:tab="nf_ssr">
					<label>Seedlings, Saplings, Rattan</label>
					<text name="species_name">
						<label>Species name</label>
					</text>
					<number name="count" type="integer" required="true">
						<label>Count</label>
						<compare gte="0" flag="error"/>
					</number>
					<!-- <taxon name="species" taxonomy="idnfi" highestRank="family" required="true"> -->
					<!-- <label>Species</label> -->
					<!-- </taxon> -->
					<number name="seedlings" type="integer">
						<label>Seedlings</label>
						<default value="0" />
						<compare gte="0" flag="error" />
					</number>
					<number name="saplings" type="integer">
						<label>Saplings</label>
						<default value="0" />
						<compare gte="0" flag="error" />
					</number>
					<number name="sm_rattan" type="integer">
						<label>Small rattan</label>
						<description>Rattan &lt;= 2.9m</description>
						<default value="0" />
						<compare gte="0" flag="error" />
					</number>
					<entity name="lg_rattan">
						<label>Large rattan</label>
						<description>Rattan &gt;=3m long</description>
						<number name="stems" type="integer">
							<label>Stems</label>
							<default value="0" />
							<compare gte="0" flag="error" />
						</number>
						<number name="max_diameter" type="real">
							<label>Max. diameter</label>
							<precision decimalDigits="1" unit="cm" />
							<compare gte="0" flag="error" />
						</number>
						<number name="min_diameter" type="real">
							<label>Min. diameter</label>
							<precision decimalDigits="1" unit="cm" />
							<compare gte="0" flag="error" />
						</number>
