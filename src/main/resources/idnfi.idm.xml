							<precision decimalDigits="1" unit="cm" />
							<compare gte="0" flag="error" />
						</number>
						<number name="avg_length" type="real">
							<label>Avg. length</label>
							<precision decimalDigits="0" unit="m" />
							<compare gte="0" flag="error" />
						</number>
