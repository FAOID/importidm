    <schema>
        <entity id="1447" name="cluster" ui:tab="cluster">
            <label type="instance">Klaster</label>            
            <number id="1330" key="true" name="utm_zone" required="true" type="integer">
                <label>UTM zone</label>
                <description>Valid UTM Zone between 46 and 54</description>
                <compare flag="error" gte="46" lte="54"/>
            </number>
            <number id="1544" key="true" name="easting" required="true" type="integer">
                <label>W-E</label>
                <description>Valid UTM easting between 166 and 834</description>
                <compare flag="error" gte="166" lte="834"/>
            </number>
            <number id="1570" key="true" name="northing" required="true" type="integer">
                <label>S-N</label>
                <description>Valid UTM northing between 0 and 750 or between 8670 and 9999</description>
                <check expr="($this &gt;= 0 and $this &lt;= 750) or ($this &gt;= 8670 and $this &lt;= 9999)" flag="error">
                    <message xml:lang="en">The northing value must be between 0 and 750 or between 8670 and 9999</message>
                    <message xml:lang="id"/>
                </check>
            </number>                        
            
            
               
